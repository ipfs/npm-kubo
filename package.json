{
  "name": "go-ipfs",
  "version": "0.9.0-rc2",
  "description": "Install the latest go-ipfs binary",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "node src/post-install.js",
    "restore-bin": "git reset -- bin/ipfs && git checkout -- bin/ipfs",
    "test": "tape test/*.js | tap-spec",
    "lint": "standard"
  },
  "pre-commit": "restore-bin",
  "bin": {
    "ipfs": "bin/ipfs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/npm-go-ipfs.git"
  },
  "keywords": [
    "ipfs",
    "install"
  ],
  "author": "Protocol Labs, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs/npm-go-ipfs/issues"
  },
  "homepage": "https://github.com/ipfs/npm-go-ipfs",
  "devDependencies": {
    "execa": "^4.0.1",
    "fs-extra": "^9.0.0",
    "pre-commit": "^1.2.2",
    "standard": "^13.1.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2",
    "tape-promise": "^4.0.0"
  },
  "dependencies": {
    "cachedir": "^2.3.0",
    "go-platform": "^1.0.0",
    "got": "^11.7.0",
    "gunzip-maybe": "^1.4.2",
    "hasha": "^5.2.2",
    "pkg-conf": "^3.1.0",
    "tar-fs": "^2.1.0",
    "unzip-stream": "^0.3.0"
  }
}
